{
    "functions": {
      "api/**": { "runtime": "vercel-php@0.7.3" }
    },
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/$1" },
      { "src": "/api/(.*)", "dest": "/api/$1" },
      { "src": "/admin/(.*)", "dest": "/api/admin/$1" },
      { "src": "/(.*)\\.php", "dest": "/api/pages/$1.php" },
      { "src": "/(.*)", "dest": "/api/$1" }
    ]
    
}

